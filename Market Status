import datetime
open_time = datetime.time(9, 15, 00, 100000)
close_time = datetime.time(15, 30, 00, 100000)
now = datetime.time()
dtnow = datetime.datetime.now()
tmnow= dtnow.time()
print(tmnow)
print(open_time)
print(close_time)
if tmnow > open_time and tmnow < close_time:
    print("Trade now")
else:
    print("Market Closed")
    
